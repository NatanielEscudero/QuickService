{
  "expo": {
    "name": "QuickService Pro",
    "slug": "quickservice-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "quickservice",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.quickservice.pro"
    },
    
    "android": {
      "package": "com.quickservice.pro",
      "adaptiveIcon": {
        "backgroundColor": "#007AFF",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "QuickService Pro",
      "shortName": "QuickService",
      "lang": "es",
      "scope": "/",
      "themeColor": "#007AFF",
      "backgroundColor": "#ffffff"
    },
    
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.584687367545-o8tunkjli3m774l3l9tesfc1fcofhob6",
          "androidClientId": "584687367545-o8tunkjli3m774l3l9tesfc1fcofhob6.apps.googleusercontent.com"
        }
      ]
    ],
    
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      }
    }
  }
}